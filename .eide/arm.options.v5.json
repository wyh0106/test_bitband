{
    "beforeBuildTasks": [],
    "afterBuildTasks": [
        {
            "name": "clean",
            "command": "del \"${OutDir}\\*._*\""
        }
    ],
    "global": {
        "use-microLIB": true,
        "output-debug-info": "enable"
    },
    "c/cpp-compiler": {
        "one-elf-section-per-function": true,
        "c99-mode": true,
        "optimization": "level-0",
        "misc-controls": [
            "--diag_suppress=1",
            "--diag_suppress=1295"
        ]
    },
    "asm-compiler": {},
    "linker": {},
    "version": 2
}